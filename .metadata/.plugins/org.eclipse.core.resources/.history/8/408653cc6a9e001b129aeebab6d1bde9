package exercicios;

public class TestaSubString {
	public static void main(String[] args) {
		
		String nome = "Ratin";
		
		for (int i = 0; i < nome.length(); i++) {
			//System.out.println(nome.substring(i));
			
			System.out.println(nome.charAt(0));
			System.out.println(nome.charAt(1));
		
		}
		
	}

}
