public class TestaMarcos {
	public static void main(String[] args) {
		
		for (int linha = 0; linha < 5; linha++) {
			for (int coluna = 0; coluna < 10; coluna++) {
				if (coluna > linha) {
					break;
			}	 
			System.out.print("m");{}
			
			System.out.print("a");
			System.out.print("r");
			
			
			}
			
		System.out.println();
		
		}
	}
}


