package exercicios;

public class DesafioFatorial {
	public static void main(String[] args) {
		
		
		int a = 1;
		for (int i = 2; i < 10; i+= 1) {
			//System.out.println(i);
			for (int j = 1; j < 10; j++) {
				System.out.println(a * i * j);
			}
			System.out.println(" ");
		}
		
			/*
			 * Fatorial 
			 * 
			 * 1! = 1x1
			 * 2! = 1x2x2
			 * 3! = 1x2x3
			 * 4! = 1x2x3x4
			 * 5! = 1x2x3x4x5
			 * 10! = 1x2x3x4x5x6x7x8x9x10
			 */
		
	}

}
