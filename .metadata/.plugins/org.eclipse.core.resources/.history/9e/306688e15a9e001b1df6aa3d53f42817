
public class TestaLacos {
	public static void main(String[] args) {
		for (int multiplicador = 1; multiplicador <= 1000; multiplicador++) {
			for (int contador = 0; contador <=1000; contador++) {
				System.out.print(multiplicador * contador);
				System.out.print(" ");
				
			}
			System.out.println();
		}
		
	}

}
