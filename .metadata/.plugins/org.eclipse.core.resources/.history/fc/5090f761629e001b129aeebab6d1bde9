
public class TestaLacos2 {

	public static void main(String[] args) {

		boolean teste = true;
		String total = "";
		
		//No for existem 3 espaços (    ;     ;    ) No primeiro espaço temos a
		
		for (int linha = 0; teste; linha++) {

			total += "#";
			System.out.println(total);

			if (linha == 10) {
				teste = false;
			}
		}

	}
}
